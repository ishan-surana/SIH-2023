{
    "name": "Scraper",
    "description": "Social Media Page Scraper",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://instagram.com/*"],
            "js": ["test.js"]
        }
    ]
}
